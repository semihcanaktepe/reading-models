var target = [5,5,3,9,4,5,6,7,1,6,3,5,3,8,6,5,5,8,2,8,8,4,6,7,6,7,4,8,4,6,2,5,3,4,10,5,9,8,7,4,6,5,4,8,6,8,5,2]

var fixate = function(word){
  // Pre-attentive visual processing (ms)
  var pavp = 50
  // Attention shift
  var mu_as = 25
  var sigma_as = mu_as*0.22
  var attention_shift = gamma(mu_as, sigma_as)
  // Familiarity check for a word (L1)
  var alpha1 = 104
  var alpha2 = 3.5
  var alpha3 = 39
  //var fam_check = bernoulli(word.predictability) ?
      //0 : alpha1 - alpha2*Math.log(word.frequency)-alpha3*word.predictability
  var fam_check = alpha1 - alpha2*Math.log(word.frequency)-alpha3*word.predictability
  // Mean time to complete familiarity check for a given word t(L1)
  var mu = fam_check
  var sigma = 0.22*mu
  var process_time = bernoulli(word.predictability) ? 0 : gamma(mu, sigma)
  // Updated familiarity check
  var epsilon = 1.15
  // Standard mean absolute deviation function
  var smad = function(word){
    var n = word.length
    // Coding where to fixate (assumes the middle of the word)
    var fixation = Math.ceil(n/2)
    return sum(map(function(x){Math.abs(fixation-x)}, _.range(1,n+1)))/n
  }
  var up_fam_check = fam_check*Math.pow(epsilon,smad(word))
  // Lexical access
  var delta = 0.34
  var lex_acc = delta*fam_check
  return {"duration": pavp + attention_shift + up_fam_check + lex_acc,
          "frequency": word.frequency}
}

// Coding saccadic programming and execution
var saccade = function(word){
  // Labile stage of saccade programming (M1), can be cancelled
  var mu_labile = 125
  var sigma_labile = 0.22*mu_labile
  var labile_time = gamma(mu_labile, sigma_labile)
  // Non-labile stage of saccadic programming (M2), cannot be cancelled
  var mu_non_labile = 25
  var sigma_non_labile = 0.22*mu_non_labile
  var non_labile_time = gamma(mu_non_labile, sigma_non_labile)
  // Since saccadic programming is completed in two stages (M1 and M2),
  // total time of a saccade is the sum of the durations of these two stages.
  var saccadic_prog = labile_time + non_labile_time
  // SACCADE LENGTH
  // Optimal saccade length in English
   var intended_saccade_length = 7
  // Range bias (systematic error) (bias)
  var psi = 7
  var omega1 = 7.3
  var omega2 = 3
  var bias = (psi - intended_saccade_length)*(omega1-Math.log(fixate(word)))/omega2
  // Random Motor error (RE)
  var eta1 = 0.5
  var eta2 = 0.15
  var random_error = gaussian(0, eta1 - eta2*intended_saccade_length)
  // Length of a saccade (S) is sum of:
  var saccade_length = intended_saccade_length + random_error + bias
  // Saccade Duration (ms)
  var sac_dur = 25
  // Probability of automatic refixation
  // Initial fixation position (IFP)
  var ifp = word.length*2
  // Optimal viewing position (OVP) (center of the word)
  var ovp = Math.ceil(word.length/2)
  var lambda = 0.16
  var prob_auto_refix = _.max([Math.abs(ifp - ovp)*lambda, 1])
  
}

// return fixation duration on a word

var read = function(text, output){
  var fix_dur = fixate(text[0])
  
  return text.length == 0 ? output : read(text)
}

var result = _.flatten(repeat(1000, function(){return map(fixate, lexicon)}))
var ten = filter(function(w) {return w.frequency < 11}, result)
var hundred = filter(function(w) {return w.frequency < 101}, result)
var thousend = filter(function(w) {return w.frequency < 1001}, result)
var tenthousend = filter(function(w) {return w.frequency < 10001}, result)
var overtenthousend = filter(function(w) {return w.frequency > 10000}, result)

var take_dur = function(d){return d.duration}
viz(map(take_dur, ten))
viz(map(take_dur, hundred))
viz(map(take_dur, thousend))
viz(map(take_dur, tenthousend))
viz(map(take_dur, overtenthousend))
